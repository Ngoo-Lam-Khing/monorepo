{
  "name": "frontend-templates",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "format": "prettier --config=$INIT_CWD/prettier.config.mjs --write $INIT_CWD",
    "format-check": "prettier --config=$INIT_CWD/prettier.config.mjs --check $INIT_CWD",
    "lint": "eslint --config=$INIT_CWD/eslint.config.mts $INIT_CWD",
    "lint-fix": "eslint --config=$INIT_CWD/eslint.config.mts $INIT_CWD --fix",
    "type-check": "tsc -p $INIT_CWD/tsconfig.json --noEmit",
    "prepare": "husky"
  },
  "bin": {
    "create-template": "tsx ./scripts/createTemplate.mts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ngoo-Lam-Khing/monorepo.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ngoo-Lam-Khing/monorepo/issues"
  },
  "homepage": "https://github.com/Ngoo-Lam-Khing/monorepo#readme",
  "description": "",
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=22.0.0",
    "yarn": ">=4.9.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/degit": "^2",
    "@types/node": "^24.3.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "lint-staged": "^16.1.5",
    "prettier": "3.6.2",
    "tsc": "^2.0.4",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vue-eslint-parser": "^10.2.0"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,ts,mts,cts,vue}": "eslint --fix",
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "degit": "^2.8.4",
    "inquirer": "^12.9.3"
  }
}
